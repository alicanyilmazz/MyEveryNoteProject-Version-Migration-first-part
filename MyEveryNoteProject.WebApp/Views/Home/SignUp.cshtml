    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    @*<link rel="stylesheet" type="text/css" href="fonts/iconic/css/material-design-iconic-font.min.css">*@
    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/fonts/iconic/css/material-design-iconic-font.min.css">
