@using MyEveryNoteProject.Entities.ValueObjects
@model SignInViewModel

@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <title>Sign IN</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    @*<link rel="icon" type="image/png" href="images/icons/favicon.ico" />*@
    <link rel="icon" type="image/png" href="~/Content/SignIn/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    @*<link rel="stylesheet" type="text/css" href="vendor/bootstrap/css/bootstrap.min.css">*@
    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/vendor/bootstrap/css/bootstrap.min.css">
    <link href="~/Content/GeneralTheme/css/ScrollBar.css" rel="stylesheet" />
    <!--===============================================================================================-->
    @*<link rel="stylesheet" type="text/css" href="fonts/font-awesome-4.7.0/css/font-awesome.min.css">*@
    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    @*<link rel="stylesheet" type="text/css" href="fonts/iconic/css/material-design-iconic-font.min.css">*@
    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    @*<link rel="stylesheet" type="text/css" href="vendor/animate/animate.css">*@
    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/vendor/animate/animate.css">
    <!--===============================================================================================-->
    @*<link rel="stylesheet" type="text/css" href="vendor/css-hamburgers/hamburgers.min.css">*@
    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    @*<link rel="stylesheet" type="text/css" href="vendor/animsition/css/animsition.min.css">*@
    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    @*<link rel="stylesheet" type="text/css" href="vendor/select2/select2.min.css">*@
    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    @*<link rel="stylesheet" type="text/css" href="vendor/daterangepicker/daterangepicker.css">*@
    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    @*<link rel="stylesheet" type="text/css" href="css/util.css">
        <link rel="stylesheet" type="text/css" href="css/main.css">*@
    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Content/SignIn/css/main.css">
    <!--===============================================================================================-->
    <link href="~/Content/SignIn/css/ValidationSummary.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link href="~/Content/SignUp/Media2.css" rel="stylesheet" />
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100" style="background-color:palegoldenrod;">
                <form action="/Home/SignIn" class="login100-form validate-form" method="post">
                    <span class="login100-form-title p-b-26 media">
                        Welcome
                    </span>
                    <span class="login100-form-title p-b-48">
                        <img src="~/images/entering2.png" />

                    </span>
                    @Html.ValidationSummary(true)
                    @*Resend Activation Mail.*@
                    @if (ViewBag.SetLink != null)
                    {
                        <div class="alert alert-warning alert-dismissible fade show" role="alert">
                            <strong><a href="@ViewBag.SetLink">We'll send you a re-activation link.!</a></strong>

                            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                    }
                    <div class="wrap-input100 " data-validate="Valid email is: a@b.c">
                        @Html.TextBoxFor(x => x.Username, new { @class = "input100", type = "text" })
                        <span class="focus-input100" data-placeholder="Username"></span>
                        <p>  @Html.ValidationMessageFor(model => model.Username, "", new { @class = "text-danger" })</p>
                    </div>

                    <div class="wrap-input100 " data-validate="Enter password">
                        <span class="btn-show-pass">
                            <i class="zmdi zmdi-eye"></i>
                        </span>
                        @Html.TextBoxFor(x => x.Password, new { @class = "input100", type = "password" })
                        <span class="focus-input100" data-placeholder="Password"></span>
                        <p>  @Html.ValidationMessageFor(model => model.Password, "", new { @class = "text-danger" })</p>
                    </div>

                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button type="submit" class="login100-form-btn">
                                Login
                            </button>
                        </div>
                    </div>

                    <div class="text-center p-t-115">
                        <span class="txt1">
                            Don’t have an account?
                        </span>

                        <a class="txt2" href="/Home/SignUp">
                            Sign Up
                        </a>
                    </div>

                    <div class="text-center p-t-115" style="padding-top:25px;">
                        <span class="txt1">
                            Would you like to go to the main page?
                        </span>

                        <a class="txt2" href="/Home/Index">
                            HomePage
                        </a>
                    </div>
                </form>
            </div>
        </div>
    </div>


    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    @*<script src="vendor/jquery/jquery-3.2.1.min.js"></script>*@
    <script src="~/Content/SignIn/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    @*<script src="vendor/animsition/js/animsition.min.js"></script>*@
    <script src="~/Content/SignIn/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    @*<script src="vendor/bootstrap/js/popper.js"></script>
        <script src="vendor/bootstrap/js/bootstrap.min.js"></script>*@
    <script src="~/Content/SignIn/vendor/bootstrap/js/popper.js"></script>
    <script src="~/Content/SignIn/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    @*<script src="vendor/select2/select2.min.js"></script>*@
    <script src="~/Content/SignIn/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    @*<script src="vendor/daterangepicker/moment.min.js"></script>
        <script src="vendor/daterangepicker/daterangepicker.js"></script>*@
    <script src="~/Content/SignIn/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/Content/SignIn/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    @*<script src="vendor/countdowntime/countdowntime.js"></script>*@
    <script src="~/Content/SignIn/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    @*<script src="js/main.js"></script>*@
    <script src="~/Content/SignIn/js/main.js"></script>


</body>
</html>
























